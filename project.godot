config_version=5

[application]

config/name="BIP-WIP"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[debug]

shapes/collision/shape_color=Color(0.886275, 0, 0.243137, 0.980392)
shapes/collision/contact_color=Color(0.752941, 0, 0.12549, 0.8)
shapes/nvidia_disable_threaded_optimization=true
shapes/overrides/force_vertex_shading.mobile=true
shapes/overrides/force_lambert_over_burley.mobile=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/quality/use_taa=false
window/quality/use_debanding=false
window/screen_space_roughness_limiter/enabled=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_nodes/plugin.cfg")

